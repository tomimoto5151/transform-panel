{
  "id": "transform-panel",
  "name": "Transform Panel",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "PS",
      "minVersion": "22.0.0"
    }
  ],
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "panel",
      "id": "transform-panel",
      "label": {
        "default": "Transform Panel"
      },
      "minimumSize": {
        "width": 240,
        "height": 300
      },
      "maximumSize": {
        "width": 240,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 240,
        "height": 600
      },
      "preferredFloatingSize": {
        "width": 240,
        "height": 600
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark@1x.png",
          "scale": [1],
          "theme": ["dark", "darkest"]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark@2x.png",
          "scale": [2],
          "theme": ["dark", "darkest"]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/dark@1x.png",
      "scale": [1],
      "theme": ["dark", "darkest"]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/dark@2x.png",
      "scale": [2],
      "theme": ["dark", "darkest"]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/light@1x.png",
      "scale": [1],
      "theme": ["light", "lightest"]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/light@2x.png",
      "scale": [2],
      "theme": ["light", "lightest"]
    }
  ]
}